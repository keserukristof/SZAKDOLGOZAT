{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "prettier",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": "off",
        "prettier/prettier": "error",
        "func-names": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "implicit-arrow-linebreak": "off",
        "react/forbid-prop-types": "off",
        "no-console": "off",
        "arrow-body-style": "off"
    }
}